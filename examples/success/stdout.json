{
  "description": "Demonstrates capturing stdout to file (and volumes).",
  "volumes": [
    "/outputs"
  ],
  "executors": [
    {
      "image": "ubuntu",
      "command": [
        "echo",
        "This will appear in stdout, but of the 2. executor."
      ],
      "stdout": "/outputs/stdout"
    },
    {
      "image": "docker-registry.ebrains.eu/tc/python:3.8-alpine",
      "command": [
        "cat",
        "/outputs/stdout"
      ]
    }
  ]
}
