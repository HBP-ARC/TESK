{
  "description": "Demonstrates handling single http file input. Will output some nice looking JSON to stdout.",
  "inputs": [
    {
      "url": "https://raw.githubusercontent.com/EMBL-EBI-TSI/TESK/master/examples/success/hello.json",
      "path": "/tes/volumes/input",
      "type": "FILE"
    }
  ],
  "executors": [
    {
      "image": "docker-registry.ebrains.eu/tc/python:3.8-alpine",
      "command": [
        "cat",
        "/tes/volumes/input"
      ]
    }
  ]
}
